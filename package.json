{
  "name": "phobik",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "format": "prettier --write src",
    "prebuild": "dotenv -e .env.${env} -- expo prebuild --clean",
    "build": "eas build -p ${platform} --local -e ${env} --non-interactive",
    "build:android": "platform=android npm run build",
    "build:ios": "platform=ios npm run build",
    "submit": "eas submit -p ${platform} -e ${env}",
    "submit:android": "platform=android yarn submit --path=$(find . -name '*.aab')",
    "submit:ios": "platform=ios yarn submit --path=$(find . -name '*.ipa')",
    "update": "eas update --channel ${env} --environment ${env} --clear-cache --auto",
    "update:preview": "env=preview yarn update"
  },
  "dependencies": {
    "@better-auth/expo": "^1.4.18",
    "@expo/vector-icons": "^15.0.3",
    "@orpc/client": "^1.13.4",
    "@orpc/react-query": "^1.13.4",
    "@orpc/server": "^1.13.4",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@tanstack/query-async-storage-persister": "^5.90.22",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-persist-client": "^5.90.22",
    "axios": "^1.13.4",
    "clsx": "^2.1.1",
    "expo": "~54.0.33",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-network": "~8.0.8",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "jotai": "^2.17.0",
    "nativewind": "^4.2.1",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.1",
    "react-native-svg-transformer": "^1.5.3",
    "typescript": "~5.9.2"
  },
  "private": true
}
